<script>
let c=document.getElementById("data").innerText
  Mydata = JSON.parse(c)
let html="";
Mydata.forEach(function(elem){
html+='<ul>'


    html+='<div>'
    html+='<li style="display=none;line-height:0px">'
html+=`<p>${elem.question}</p><br>`;
html+=`<p>${elem.option1}</p><br>`;
html+=`<p>${elem.option2}</p><br>`;
html+=`<p>${elem.option3}</p><br>`;
html+=`<p>${elem.option4}</p><br>`;4
html+='</li>'
html+='</div>'

html+='</ul>'
})
document.getElementById("mydata").innerHTML=html
</script>